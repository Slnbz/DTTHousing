<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.android.dtthousing.ui.main.view.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/infoActivity"
        android:name="com.example.android.dtthousing.ui.main.view.InfoActivity"
        android:label="info_activity"
        tools:layout="@layout/activity_info" />
    <activity
        android:id="@+id/emptyScreenActivity"
        android:name="com.example.android.dtthousing.ui.main.view.EmptyScreenActivity"
        android:label="empty_activity"
        tools:layout="@layout/activity_empty_search" />
</navigation>